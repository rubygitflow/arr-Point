<% if @car.persisted? && @car.errors.present? %>
  $('#new-car-errors').html('<%= j render 'shared/errors', resource: @car %>');
<% end %>

<% if @car.persisted? %>
  $('.cars').append('<%= j render @car  %>');
  $('.new-car #car_pictures').val([]);
  $('.new-car #car_license_plate').val('');
  $('.new-car #car_model').val('');
  $('.new-car #car_year_manufacture').val('');
  $(".new-car #car_pictures").prop('disabled', false);
<% end %>
